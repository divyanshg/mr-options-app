    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.divg809.mroptions",
      "permissions": [
        "INTERNET"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fc2ce145-bb08-4bd5-bf9a-081e9ee9df41"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
